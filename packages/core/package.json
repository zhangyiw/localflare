{
  "name": "localflare-core",
  "version": "0.4.2",
  "description": "Core utilities for Localflare - config parsing and binding discovery",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "tiny-jsonc": "^1.0.2",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "wrangler",
    "config",
    "local",
    "development"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rohanprasadofficial/localflare",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/rohanprasadofficial/localflare#readme",
  "publishConfig": {
    "access": "public"
  }
}
