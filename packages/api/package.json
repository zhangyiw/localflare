{
  "name": "localflare-api",
  "version": "0.4.2",
  "description": "API worker for Localflare dashboard - runs alongside your worker with shared bindings",
  "type": "module",
  "main": "./dist/worker/index.js",
  "exports": {
    ".": {
      "import": "./dist/worker/index.js",
      "default": "./dist/worker/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "localflare-core": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "dashboard",
    "api",
    "localflare",
    "d1",
    "kv",
    "r2",
    "queues"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rohanprasadofficial/localflare",
    "directory": "packages/api"
  },
  "homepage": "https://github.com/rohanprasadofficial/localflare#readme",
  "publishConfig": {
    "access": "public"
  }
}
